{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "A React modern seed using webpack 2 and yarn package manager",
  "main": "index.js",
  "author": "Eduardo Gardin",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --env.prod=true",
    "clean": "rimraf dist",
    "serve.dev": "webpack-dev-server --hot",
    "test": "mocha ./test-setup.js ./src/**/*.spec.js",
    "test.watch": "mocha -w ./test-setup.js ./src/**/*.spec.js"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "chai": "^3.5.0",
    "cross-env": "^3.1.4",
    "enzyme": "^2.7.1",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^9.9.1",
    "mocha": "^3.2.0",
    "react-addons-test-utils": "^15.4.2",
    "rimraf": "^2.5.4",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^1.16.3"
  }
}
